{
  "name": "openvpn-manager",
  "homepage": "https://github.com/shhserver/openvpn-manager",
  "description": "A web-based application to monitor clients connected to an openvpn server.",
  "version": "4.2.30",
  "license": "MIT",
  "main": "server.js",
  "author": {
    "name": "Arun Panneerselvam",
    "email": "me@arunpanneerselvam.com",
    "url": "https://ovpnman.arunpanneerselvam.com"
  },
  "engines": {
    "node": ">=8.9"
  }
  "husky": {
    "hooks": {
      "pre-push": "npm run deps && npm run ncu"
    }
  },
  "keywords": [
    "Shh Server",
    "Opensource node express server",
    "node express server with dashboard"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/cosmoarunn/shhserver/shhserver.git"
  },
  "bugs": {
    "url": "https://github.com/cosmoarunn/shhserver/shhserver.git"
  },
  "scripts": {
    "start": "node server/server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deps": "depcheck . --ignores=\"husky,sqlite3,@vue/*,eslint-plugin-import,babel-eslint,vue-template-compiler\"",
    "ncu": "ncu"
  },
  "dependencies": {
    "apollo-server": "^2.16.1",
    "apollo-server-express": "^2.16.1",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "bootstrap-vue": "^2.16.0",
    "chalk": "^4.1.0",
    "express": "4.17.1",
    "graphql-server-express": "^1.4.1",
    "http-proxy-middleware": "^1.0.1",
    "isemail": "^3.2.0",
    "maxmind": "4.1.1",
    "moment": "2.24.0",
    "nconf": "0.10.0",
    "reconnecting-websocket": "^4.4.0",
    "sequelize": "5.21.5",
    "sqlite3": "^5.0.0",
    "subscriptions-transport-ws": "^0.9.18",
    "uuid": "7.0.2",
    "vue": "2.6.11",
    "vue-router": "3.1.6",
    "vuetify": "2.2.19",
    "vuex": "3.1.3",
    "ws": "7.2.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "4.2.3",
    "@vue/cli-service": "4.2.3",
    "@vue/eslint-config-airbnb": "5.0.2",
    "async": "^3.2.0",
    "babel-eslint": "10.1.0",
    "depcheck": "0.9.2",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "6.2.2",
    "husky": "4.2.3",
    "npm-check-updates": "4.1.0",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "L": true,
      "ReconnectingWebSocket": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-mixed-operators": 0,
      "max-len": 0,
      "no-plusplus": 0,
      "no-return-assign": 0,
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "import/no-extraneous-dependencies": 0,
      "nonblock-statement-body-position": [
        "error",
        "below"
      ],
      "curly": [
        "error",
        "multi"
      ],
      "no-new-require": 0,
      "new-cap": 0,
      "semi": 0,
      "no-console": [
        "error",
        {
          "allow": [
            "log",
            "error"
          ]
        }
      ],
      "object-curly-spacing": 0,
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "_"
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "consistent-return": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "maintainers": [
    {
      "name": "Arun Panneerselvam",
      "email": "me@arunpanneerselvam.com"
    }
  ]
}
